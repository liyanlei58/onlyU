<home wx:if="{{PageCur=='home'}}"></home>
<apply wx:if="{{PageCur=='apply'}}"></apply>
<seek wx:if="{{PageCur=='seek'}}"></seek>
<dynamic wx:if="{{PageCur=='dynamic'}}"></dynamic>
<chat wx:if="{{PageCur=='chat'}}"></chat>
<me wx:if="{{PageCur=='me'}}"></me>


<view class="cu-bar tabbar bg-white foot">

  <view class="action" bindtap="NavChange" data-cur="home">
    <view class='cuIcon-cu-image'>
    <text class="cuIcon-home{{PageCur=='home'?('fill text-' + skin):' text-gray'}}"></text>
    </view>
    <view class="{{PageCur=='home'?('text-' + skin):'text-gray'}}">首页</view>
  </view>
